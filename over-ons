<!doctype html>
<html>
  <head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width,initial-scale=1">
    <title>van Soest bouw - Over ons</title>
    <script>
      (function(){
        fetch('/layout.html').then(function(r){ if(!r.ok) throw new Error('layout fetch failed'); return r.text(); }).then(function(html){ document.open(); document.write(html); document.close(); }).catch(function(e){ console.error(e); document.body.innerHTML = '<p>Could not load layout.</p>'; });
      })();
    </script>
  </head>
  <body></body>
</html>
