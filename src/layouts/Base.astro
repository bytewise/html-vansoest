---
import { ViewTransitions } from 'astro:transitions';
import { Image } from '@astrojs/image/components';
import Navigation from '../components/Navigation.astro';
const { title = 'van Soest bouw', description = 'Van Soest Bouw — vakbekwaam bouwen, renoveren en verbouwen. Neem contact op voor een vrijblijvende offerte.' } = Astro.props;
